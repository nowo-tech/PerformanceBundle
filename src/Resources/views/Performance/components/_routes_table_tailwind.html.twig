{# Routes Table Component (Tailwind) #}
{# Expects: routes, sortBy, order (for sortable headers), paginator (optional). #}
{% set _query = app.request.query.all|default({}) %}
<div class="bg-white shadow rounded-lg">
    <div class="px-4 py-5 sm:p-6">
        <h3 class="text-lg leading-6 font-medium text-gray-900 mb-4">{{ 'routes_table.title'|trans({}, 'nowo_performance') }} ({% if paginator is defined and paginator.total_count is defined %}{{ paginator.total_count }}{% else %}{{ routes|length }}{% endif %})</h3>
        {% if routes is empty %}
            <div class="rounded-md bg-blue-50 p-4">
                <div class="flex">
                    <div class="flex-shrink-0">
                        {{ ux_icon('bi:info-circle', {class: 'h-5 w-5 text-blue-400'}) }}
                    </div>
                    <div class="ml-3">
                        <p class="text-sm text-blue-700">No routes found for the selected filters.</p>
                    </div>
                </div>
            </div>
        {% else %}
            <div class="overflow-x-auto">
                <table class="min-w-full divide-y divide-gray-200">
                    <thead class="bg-gray-50">
                        <tr>
                            <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"><a href="{{ path('nowo_performance.index', _query|merge({sort: 'name', order: (sortBy|default('') == 'name' and (order|default('DESC')) == 'DESC') ? 'ASC' : 'DESC', page: 1})) }}" class="text-gray-600 hover:text-gray-900">{{ 'routes_table.route_name'|trans({}, 'nowo_performance') }}{% if sortBy|default('') == 'name' %} {% if order|default('DESC') == 'DESC' %}↓{% else %}↑{% endif %}{% endif %}</a></th>
                            <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">{{ 'routes_table.http_method'|trans({}, 'nowo_performance') }}</th>
                            {% if trackStatusCodes is defined and trackStatusCodes is not empty %}
                                <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">{{ 'routes_table.status_codes'|trans({}, 'nowo_performance') }}</th>
                            {% endif %}
                            <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"><a href="{{ path('nowo_performance.index', _query|merge({sort: 'accessCount', order: (sortBy|default('') == 'accessCount' and (order|default('DESC')) == 'DESC') ? 'ASC' : 'DESC', page: 1})) }}" class="text-gray-600 hover:text-gray-900" title="{{ (sortBy|default('') == 'accessCount' and order|default('DESC') == 'ASC') ? 'routes_table.sort_descending'|trans({}, 'nowo_performance') : 'routes_table.sort_ascending'|trans({}, 'nowo_performance') }}">{{ 'routes_table.access_count'|trans({}, 'nowo_performance') }}{% if sortBy|default('') == 'accessCount' %} {% if order|default('DESC') == 'DESC' %}↓{% else %}↑{% endif %}{% endif %}</a></th>
                            <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">{{ 'routes_table.environment'|trans({}, 'nowo_performance') }}</th>
                            <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"><a href="{{ path('nowo_performance.index', _query|merge({sort: 'requestTime', order: (sortBy|default('') == 'requestTime' and (order|default('DESC')) == 'DESC') ? 'ASC' : 'DESC', page: 1})) }}" class="text-gray-600 hover:text-gray-900" title="{{ (sortBy|default('') == 'requestTime' and order|default('DESC') == 'ASC') ? 'routes_table.sort_descending'|trans({}, 'nowo_performance') : 'routes_table.sort_ascending'|trans({}, 'nowo_performance') }}">{{ 'routes_table.request_time'|trans({}, 'nowo_performance') }}{% if sortBy|default('') == 'requestTime' %} {% if order|default('DESC') == 'DESC' %}↓{% else %}↑{% endif %}{% endif %}</a></th>
                            <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"><a href="{{ path('nowo_performance.index', _query|merge({sort: 'queryTime', order: (sortBy|default('') == 'queryTime' and (order|default('DESC')) == 'DESC') ? 'ASC' : 'DESC', page: 1})) }}" class="text-gray-600 hover:text-gray-900" title="{{ (sortBy|default('') == 'queryTime' and order|default('DESC') == 'ASC') ? 'routes_table.sort_descending'|trans({}, 'nowo_performance') : 'routes_table.sort_ascending'|trans({}, 'nowo_performance') }}">{{ 'routes_table.query_time'|trans({}, 'nowo_performance') }}{% if sortBy|default('') == 'queryTime' %} {% if order|default('DESC') == 'DESC' %}↓{% else %}↑{% endif %}{% endif %}</a></th>
                            <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"><a href="{{ path('nowo_performance.index', _query|merge({sort: 'totalQueries', order: (sortBy|default('') == 'totalQueries' and (order|default('DESC')) == 'DESC') ? 'ASC' : 'DESC', page: 1})) }}" class="text-gray-600 hover:text-gray-900" title="{{ (sortBy|default('') == 'totalQueries' and order|default('DESC') == 'ASC') ? 'routes_table.sort_descending'|trans({}, 'nowo_performance') : 'routes_table.sort_ascending'|trans({}, 'nowo_performance') }}">{{ 'routes_table.total_queries'|trans({}, 'nowo_performance') }}{% if sortBy|default('') == 'totalQueries' %} {% if order|default('DESC') == 'DESC' %}↓{% else %}↑{% endif %}{% endif %}</a></th>
                            <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"><a href="{{ path('nowo_performance.index', _query|merge({sort: 'memoryUsage', order: (sortBy|default('') == 'memoryUsage' and (order|default('DESC')) == 'DESC') ? 'ASC' : 'DESC', page: 1})) }}" class="text-gray-600 hover:text-gray-900" title="{{ (sortBy|default('') == 'memoryUsage' and order|default('DESC') == 'ASC') ? 'routes_table.sort_descending'|trans({}, 'nowo_performance') : 'routes_table.sort_ascending'|trans({}, 'nowo_performance') }}">{{ 'routes_table.memory_usage'|trans({}, 'nowo_performance') }}{% if sortBy|default('') == 'memoryUsage' %} {% if order|default('DESC') == 'DESC' %}↓{% else %}↑{% endif %}{% endif %}</a></th>
                            <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"><a href="{{ path('nowo_performance.index', _query|merge({sort: 'lastAccessedAt', order: (sortBy|default('') == 'lastAccessedAt' and (order|default('DESC')) == 'DESC') ? 'ASC' : 'DESC', page: 1})) }}" class="text-gray-600 hover:text-gray-900" title="{{ (sortBy|default('') == 'lastAccessedAt' and order|default('DESC') == 'ASC') ? 'routes_table.sort_descending'|trans({}, 'nowo_performance') : 'routes_table.sort_ascending'|trans({}, 'nowo_performance') }}">{{ 'routes_table.last_accessed_at'|trans({}, 'nowo_performance') }}{% if sortBy|default('') == 'lastAccessedAt' %} {% if order|default('DESC') == 'DESC' %}↓{% else %}↑{% endif %}{% endif %}</a></th>
                            {% if enableAccessRecords is defined and enableAccessRecords %}
                                <th scope="col" class="px-6 py-3 text-center text-xs font-medium text-gray-500 uppercase tracking-wider">{{ 'access_statistics.records'|trans({}, 'nowo_performance') }}</th>
                            {% endif %}
                            {% if enableReviewSystem is defined and enableReviewSystem %}
                                <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">{{ 'routes_table.review_status'|trans({}, 'nowo_performance') }}</th>
                                <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">{{ 'routes_table.queries_time'|trans({}, 'nowo_performance') }}</th>
                            {% endif %}
                            {% if (enableRecordManagement is defined and enableRecordManagement) or (enableReviewSystem is defined and enableReviewSystem) %}
                                <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">{{ 'routes_table.actions'|trans({}, 'nowo_performance') }}</th>
                            {% endif %}
                        </tr>
                    </thead>
                    <tbody class="bg-white divide-y divide-gray-200">
                        {% for route in routes %}
                            <tr class="hover:bg-gray-50">
                                <td class="px-6 py-4 whitespace-nowrap">
                                    <code class="text-sm text-gray-900">{{ route.name ?? 'N/A' }}</code>
                                </td>
                                <td class="px-6 py-4 whitespace-nowrap">
                                    {% if route.httpMethod %}
                                        {% set methodColors = {
                                            'GET': 'bg-blue-100 text-blue-800',
                                            'POST': 'bg-green-100 text-green-800',
                                            'PUT': 'bg-yellow-100 text-yellow-800',
                                            'PATCH': 'bg-cyan-100 text-cyan-800',
                                            'DELETE': 'bg-red-100 text-red-800',
                                            'OPTIONS': 'bg-gray-100 text-gray-800',
                                            'HEAD': 'bg-gray-200 text-gray-900'
                                        } %}
                                        <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full {{ methodColors[route.httpMethod]|default('bg-gray-100 text-gray-800') }}">{{ route.httpMethod }}</span>
                                    {% else %}
                                        <span class="text-gray-400">N/A</span>
                                    {% endif %}
                                </td>
                                {% if trackStatusCodes is defined and trackStatusCodes is not empty %}
                                    <td class="px-6 py-4 whitespace-nowrap">
                                        {% if route.getStatusCodes() is not empty %}
                                            {% set totalResponses = route.getTotalResponses() %}
                                            {% set statusList = [] %}
                                            {% for code, count in route.getStatusCodes() %}
                                                {% set ratio = totalResponses > 0 ? (count / totalResponses) * 100 : 0 %}
                                                {% set statusList = statusList|merge([{code: code, count: count, ratio: ratio}]) %}
                                            {% endfor %}
                                            {% set statusList = statusList|sort((a, b) => b.count <=> a.count) %}
                                            <div class="flex flex-wrap gap-1">
                                                {% for item in statusList %}
                                                    {% set badgeClass = 'bg-gray-100 text-gray-800' %}
                                                    {% if item.code >= 200 and item.code < 300 %}
                                                        {% set badgeClass = 'bg-green-100 text-green-800' %}
                                                    {% elseif item.code >= 300 and item.code < 400 %}
                                                        {% set badgeClass = 'bg-blue-100 text-blue-800' %}
                                                    {% elseif item.code >= 400 and item.code < 500 %}
                                                        {% set badgeClass = 'bg-yellow-100 text-yellow-800' %}
                                                    {% elseif item.code >= 500 %}
                                                        {% set badgeClass = 'bg-red-100 text-red-800' %}
                                                    {% endif %}
                                                    <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full {{ badgeClass }}" title="{{ item.count }} / {{ totalResponses }} ({{ item.ratio|number_format(1) }}%)">
                                                        {{ item.code }}: {{ item.ratio|number_format(1) }}% ({{ item.count }})
                                                    </span>
                                                {% endfor %}
                                            </div>
                                        {% else %}
                                            <span class="text-sm text-gray-500">N/A</span>
                                        {% endif %}
                                    </td>
                                {% endif %}
                                <td class="px-6 py-4 whitespace-nowrap">
                                    <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-blue-100 text-blue-800" title="Number of times this route has been accessed">
                                        {{ route.getAccessCount()|default(1)|number_format(0) }}
                                    </span>
                                </td>
                                <td class="px-6 py-4 whitespace-nowrap">
                                    <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-gray-100 text-gray-800">
                                        {{ route.env ?? 'N/A' }}
                                    </span>
                                </td>
                                <td class="px-6 py-4 whitespace-nowrap">
                                    {% if route.requestTime is not null %}
                                        {% set thresholds = thresholds|default({}) %}
                                        {% set requestTimeWarning = (thresholds.request_time.warning|default(0.5)) * 1000 %}
                                        {% set requestTimeCritical = (thresholds.request_time.critical|default(1.0)) * 1000 %}
                                        {% set timeMs = route.requestTime * 1000 %}
                                        {% if timeMs > requestTimeCritical %}
                                            <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-red-100 text-red-800">
                                                {{ timeMs|number_format(2) }} ms
                                            </span>
                                        {% elseif timeMs > requestTimeWarning %}
                                            <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-yellow-100 text-yellow-800">
                                                {{ timeMs|number_format(2) }} ms
                                            </span>
                                        {% else %}
                                            <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-green-100 text-green-800">
                                                {{ timeMs|number_format(2) }} ms
                                            </span>
                                        {% endif %}
                                    {% else %}
                                        <span class="text-sm text-gray-500">N/A</span>
                                    {% endif %}
                                </td>
                                <td class="px-6 py-4 whitespace-nowrap">
                                    {% if route.queryTime is not null %}
                                        <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-cyan-100 text-cyan-800">
                                            {{ (route.queryTime * 1000)|number_format(2) }} ms
                                        </span>
                                    {% else %}
                                        <span class="text-sm text-gray-500">N/A</span>
                                    {% endif %}
                                </td>
                                <td class="px-6 py-4 whitespace-nowrap">
                                    {% if route.totalQueries is not null %}
                                        {% set thresholds = thresholds|default({}) %}
                                        {% set queryCountWarning = thresholds.query_count.warning|default(20) %}
                                        {% set queryCountCritical = thresholds.query_count.critical|default(50) %}
                                        {% if route.totalQueries > queryCountCritical %}
                                            <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-red-100 text-red-800">
                                                {{ route.totalQueries }}
                                            </span>
                                        {% elseif route.totalQueries > queryCountWarning %}
                                            <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-yellow-100 text-yellow-800">
                                                {{ route.totalQueries }}
                                            </span>
                                        {% else %}
                                            <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-green-100 text-green-800">
                                                {{ route.totalQueries }}
                                            </span>
                                        {% endif %}
                                    {% else %}
                                        <span class="text-sm text-gray-500">N/A</span>
                                    {% endif %}
                                </td>
                                <td class="px-6 py-4 whitespace-nowrap">
                                    {% if route.memoryUsage is not null %}
                                        {% set thresholds = thresholds|default({}) %}
                                        {% set memoryUsageWarning = thresholds.memory_usage.warning|default(20.0) %}
                                        {% set memoryUsageCritical = thresholds.memory_usage.critical|default(50.0) %}
                                        {% set memoryMB = route.memoryUsage / 1024 / 1024 %}
                                        {% if memoryMB > memoryUsageCritical %}
                                            <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-red-100 text-red-800">
                                                {{ memoryMB|number_format(2) }} MB
                                            </span>
                                        {% elseif memoryMB > memoryUsageWarning %}
                                            <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-yellow-100 text-yellow-800">
                                                {{ memoryMB|number_format(2) }} MB
                                            </span>
                                        {% else %}
                                            <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-cyan-100 text-cyan-800">
                                                {{ memoryMB|number_format(2) }} MB
                                            </span>
                                        {% endif %}
                                    {% else %}
                                        <span class="text-sm text-gray-500">N/A</span>
                                    {% endif %}
                                </td>
                                <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                                    {% if route.getLastAccessedAt() %}
                                        {{ route.getLastAccessedAt()|date(dateTimeFormat|default('Y-m-d H:i:s')) }}
                                    {% else %}
                                        <span class="text-gray-400">N/A</span>
                                    {% endif %}
                                </td>
                                {% if enableAccessRecords is defined and enableAccessRecords %}
                                    <td class="px-6 py-4 whitespace-nowrap text-center">
                                        {% if route.name %}
                                            <a href="{{ path('nowo_performance.access_records', {
                                                route: route.name,
                                                env: environment|default('dev')
                                            }) }}"
                                               class="inline-flex items-center px-2 py-1 border border-gray-300 rounded-md text-xs font-medium text-gray-700 bg-white hover:bg-gray-50"
                                               title="View access records for this route">
                                                {{ ux_icon('bi:eye', {class: 'w-4 h-4'}) }}
                                            </a>
                                        {% else %}
                                            <span class="text-sm text-gray-400">-</span>
                                        {% endif %}
                                    </td>
                                {% endif %}
                                {% if enableReviewSystem is defined and enableReviewSystem %}
                                    <td class="px-6 py-4 whitespace-nowrap text-sm">
                                        {% if route.isReviewed() %}
                                            <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-green-100 text-green-800">{{ 'routes_table.reviewed'|trans({}, 'nowo_performance') }}</span>
                                            {% if route.getReviewedAt() %}
                                                <div class="mt-1 text-xs text-gray-500">{{ route.getReviewedAt()|date(dateFormat|default('Y-m-d H:i')) }}</div>
                                            {% endif %}
                                        {% else %}
                                            <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-gray-100 text-gray-800">{{ 'routes_table.not_reviewed'|trans({}, 'nowo_performance') }}</span>
                                        {% endif %}
                                    </td>
                                    <td class="px-6 py-4 whitespace-nowrap text-sm">
                                        {% if route.getQueriesImproved() is not null %}
                                            <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full {{ route.getQueriesImproved() ? 'bg-green-100 text-green-800' : 'bg-red-100 text-red-800' }}">
                                                {{ route.getQueriesImproved() ? 'routes_table.queries_improved'|trans({}, 'nowo_performance') : 'routes_table.queries_not_improved'|trans({}, 'nowo_performance') }}
                                            </span>
                                            {% if route.getTimeImproved() is not null %}<br class="mt-1">{% endif %}
                                        {% endif %}
                                        {% if route.getTimeImproved() is not null %}
                                            <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full {{ route.getTimeImproved() ? 'bg-green-100 text-green-800' : 'bg-red-100 text-red-800' }}">
                                                {{ route.getTimeImproved() ? 'routes_table.time_improved'|trans({}, 'nowo_performance') : 'routes_table.time_not_improved'|trans({}, 'nowo_performance') }}
                                            </span>
                                        {% endif %}
                                        {% if route.getQueriesImproved() is null and route.getTimeImproved() is null %}
                                            <span class="text-gray-400">—</span>
                                        {% endif %}
                                    </td>
                                {% endif %}
                                {% if (enableRecordManagement is defined and enableRecordManagement) or (enableReviewSystem is defined and enableReviewSystem) %}
                                    <td class="px-6 py-4 whitespace-nowrap text-sm font-medium">
                                        <div class="flex space-x-2">
                                            {% if enableRecordManagement is defined and enableRecordManagement and deleteForms[route.id] is defined %}
                                                {% set deleteForm = deleteForms[route.id] %}
                                                {{ form_start(deleteForm, {'action': path('nowo_performance.delete', {id: route.id}), 'attr': {'style': 'display: inline;', 'onsubmit': "return confirm('" ~ ('confirmations.delete_record'|trans({}, 'nowo_performance')) ~ "');"}}) }}
                                                    {{ form_widget(deleteForm._token) }}
                                                    <button type="submit" class="text-red-600 hover:text-red-900" title="{{ 'routes_table.delete'|trans({}, 'nowo_performance') }}">
                                                        {{ ux_icon('bi:trash', {class: 'w-4 h-4'}) }}
                                                    </button>
                                                {{ form_end(deleteForm) }}
                                            {% endif %}
                                            {% if enableReviewSystem is defined and enableReviewSystem and reviewForms[route.id] is defined %}
                                                {% set reviewForm = reviewForms[route.id] %}
                                                <button type="button" class="text-blue-600 hover:text-blue-900" onclick="document.getElementById('reviewModal{{ route.id }}').classList.remove('hidden')" title="{{ route.isReviewed() ? 'routes_table.edit_review'|trans({}, 'nowo_performance') : 'routes_table.mark_as_reviewed'|trans({}, 'nowo_performance') }}">
                                                    {{ ux_icon(route.isReviewed() ? 'bi:pencil-square' : 'bi:check-circle', {class: 'w-4 h-4'}) }}
                                                </button>
                                                <div id="reviewModal{{ route.id }}" class="hidden fixed inset-0 z-50 flex min-h-screen items-center justify-center overflow-y-auto bg-gray-600/50 p-4" onclick="if (event.target === this) this.classList.add('hidden')">
                                                    <div class="relative w-full max-w-md max-h-[90vh] overflow-y-auto rounded-lg border border-gray-200 bg-white p-5 shadow-lg" onclick="event.stopPropagation()">
                                                        {{ form_start(reviewForm, {'action': path('nowo_performance.review', {id: route.id})}) }}
                                                            <div class="flex justify-between items-center mb-4">
                                                                <h3 class="text-lg font-medium">{{ (route.isReviewed() ? 'review.modal_title_edit' : 'review.modal_title')|trans({}, 'nowo_performance') }}</h3>
                                                                    <button type="button" onclick="document.getElementById('reviewModal{{ route.id }}').classList.add('hidden')" class="text-gray-400 hover:text-gray-600">
                                                                        {{ ux_icon('bi:x-lg', {class: 'w-5 h-5'}) }}
                                                                    </button>
                                                                </div>
                                                                <div class="mb-4">
                                                                    {% if not reviewForm.queries_improved.isRendered %}
                                                                        {{ form_label(reviewForm.queries_improved, null, {'attr': {'class': 'block text-sm font-medium text-gray-700 mb-1'}}) }}
                                                                        {{ form_widget(reviewForm.queries_improved, {'attr': {'class': 'mt-1 block w-full rounded-md border-gray-300 shadow-sm focus:border-indigo-500 focus:ring-indigo-500 sm:text-sm'}}) }}
                                                                        {{ form_errors(reviewForm.queries_improved) }}
                                                                    {% endif %}
                                                                </div>
                                                                <div class="mb-4">
                                                                    {% if not reviewForm.time_improved.isRendered %}
                                                                        {{ form_label(reviewForm.time_improved, null, {'attr': {'class': 'block text-sm font-medium text-gray-700 mb-1'}}) }}
                                                                        {{ form_widget(reviewForm.time_improved, {'attr': {'class': 'mt-1 block w-full rounded-md border-gray-300 shadow-sm focus:border-indigo-500 focus:ring-indigo-500 sm:text-sm'}}) }}
                                                                        {{ form_errors(reviewForm.time_improved) }}
                                                                    {% endif %}
                                                                </div>
                                                                <div class="flex justify-end space-x-2">
                                                                    <button type="button" onclick="document.getElementById('reviewModal{{ route.id }}').classList.add('hidden')" class="px-4 py-2 bg-gray-200 text-gray-800 rounded hover:bg-gray-300">{{ 'routes_table.cancel'|trans({}, 'nowo_performance') }}</button>
                                                                    {% if not reviewForm.submit.isRendered %}
                                                                        {{ form_widget(reviewForm.submit, {'attr': {'class': 'px-4 py-2 bg-blue-600 text-white rounded hover:bg-blue-700'}}) }}
                                                                    {% endif %}
                                                                </div>
                                                            {{ form_end(reviewForm) }}
                                                        </div>
                                                    </div>
                                            {% endif %}
                                        </div>
                                    </td>
                                {% endif %}
                            </tr>
                        {% endfor %}
                    </tbody>
                </table>
            </div>
        {% endif %}
    </div>
</div>
